<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE TimeTable [
    <!ELEMENT TimeTable (complexes, lessons)>
    <!ELEMENT complexes (complexe+) >
    <!ELEMENT complexe (name, adress)>
    <!ELEMENT lessons (lesson+) >
    <!ELEMENT lesson (date, time)>
    <!ELEMENT name (#PCDATA)>
    <!ELEMENT adress (#PCDATA)>
    <!ELEMENT date (#PCDATA)>
    <!ELEMENT time (#PCDATA)>
    <!ATTLIST complexe code ID #REQUIRED>
    <!ATTLIST lesson 
        no CDATA #REQUIRED
        complex IDREF #REQUIRED
    >
]>
<TimeTable>
    <complexes>
